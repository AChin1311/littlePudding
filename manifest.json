{
  "manifest_version": 2,

    "name": "Photo shooting",
  "description": "This extension shows an Image from your mobile phone",
  "version": "1.0",
  "icons": { "128": "icon.png" },
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab"
  ]
}